{
  "name": "@open51/hooks",
  "version": "1.0.2",
  "description": "React hooks",
  "keywords": [
    "react",
    "hooks"
  ],
  "main": "index",
  "types": "index",
  "license": "MIT",
  "scripts": {
    "dev": "yarn rmrf && tsc -W -p tsconfig.json",
