    "build:copy-files": "node ../../scripts/copy-files.js",
    "release": "yarn build && npm publish dist --access public",
    "rmrf": "rimraf dist",
    "pretty": "prettier --write src/**/*"
  },
  "dependencies": {
    "@open51/utils": "^1.0.2"
  },
  "peerDependencies": {
    "react": ">= 16.8.0"
  }
}
